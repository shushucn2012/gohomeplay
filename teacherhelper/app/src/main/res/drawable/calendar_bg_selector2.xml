<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item app:state_range_middle="true">
        <color android:color="@color/color_text_red_date_range" />
    </item>
    <item android:state_selected="true">
        <color android:color="@color/color_text_red_deep" />
    </item>
    <item android:state_pressed="true">
        <color android:color="@color/color_text_red_deep" />
    </item>
    <item app:state_current_month="false">
        <color android:color="@color/gffffff" />
    </item>
    <item app:state_highlighted="true">
        <color android:color="@color/calendar_highlighted_day_bg" />
    </item>
    <item app:state_today="true">
        <color android:color="@color/color_text_red_light" />
    </item>
    <item>
        <color android:color="@color/gffffff" />
    </item>
</selector>
